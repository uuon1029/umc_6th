<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/find_id_result_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="아이디 찾기"
        android:textColor="@color/black"
        android:textSize="25sp"
        android:textStyle="bold"
        android:layout_marginTop="90dp"
        android:layout_marginStart="20dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:id="@+id/find_id_result_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="휴대폰 번호 정보와 일치하는 아이디입니다."
        android:textColor="@color/black"
        android:textSize="14sp"
        android:layout_marginTop="30dp"
        app:layout_constraintTop_toBottomOf="@id/find_id_result_title"
        app:layout_constraintStart_toStartOf="@id/find_id_result_title"/>
    
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="173dp"
        android:background="@drawable/bg_rectangle_main_radius_30"
        android:layout_marginTop="20dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toBottomOf="@id/find_id_result_tv"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TTungTTang77"
        android:textSize="25sp"



    <TextView
        android:id="@+id/find_id_result_btn"
        android:layout_width="310dp"
        android:layout_height="45dp"
        android:text="아이디 찾기"
        android:textSize="17sp"
        android:textColor="@color/main_color"
        android:textStyle="bold"
        android:gravity="center"
        android:background="@drawable/bg_rectangle_black_radius_23"
        android:layout_marginBottom="30dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>



</androidx.constraintlayout.widget.ConstraintLayout>