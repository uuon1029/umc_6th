<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/gray20">

    <ImageButton
        android:id="@+id/admin_home_quest_ib"
        android:layout_width="320dp"
        android:layout_height="63dp"
        android:layout_marginTop="83dp"
        android:background="@drawable/bg_config_option1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/admin_home_quest_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="14sp"
        android:text="자주 묻는 질문"
        android:textStyle="bold"
        android:textColor="@color/black"
        android:layout_marginStart="20dp"
        app:layout_constraintTop_toTopOf="@id/admin_home_quest_ib"
        app:layout_constraintBottom_toBottomOf="@id/admin_home_quest_ib"
        app:layout_constraintStart_toStartOf="@id/admin_home_quest_ib"/>

    <View
        android:layout_width="303dp"
        android:layout_height="1px"
        android:background="@color/gray40"
        app:layout_constraintStart_toStartOf="@id/admin_home_quest_ib"
        app:layout_constraintEnd_toEndOf="@id/admin_home_quest_ib"
        app:layout_constraintBottom_toBottomOf="@id/admin_home_quest_ib"/>

    <ImageButton
        android:id="@+id/admin_home_1to1_ib"
        android:layout_width="320dp"
        android:layout_height="63dp"
        android:background="@drawable/bg_config_option2"
        app:layout_constraintStart_toStartOf="@id/admin_home_quest_ib"
        app:layout_constraintTop_toBottomOf="@id/admin_home_quest_ib" />

    <TextView
        android:id="@+id/admin_home_1to1_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="14sp"
        android:text="1:1 문의"
        android:textStyle="bold"
        android:textColor="@color/black"
        android:layout_marginStart="20dp"
        app:layout_constraintTop_toTopOf="@id/admin_home_1to1_ib"
        app:layout_constraintBottom_toBottomOf="@id/admin_home_1to1_ib"
        app:layout_constraintStart_toStartOf="@id/admin_home_1to1_ib"/>

    <View
        android:layout_width="303dp"
        android:layout_height="1px"
        android:background="@color/gray40"
        app:layout_constraintStart_toStartOf="@id/admin_home_1to1_ib"
        app:layout_constraintEnd_toEndOf="@id/admin_home_1to1_ib"
        app:layout_constraintBottom_toBottomOf="@id/admin_home_1to1_ib"/>

    <ImageButton
        android:id="@+id/admin_home_report_ib"
        android:layout_width="320dp"
        android:layout_height="63dp"
        android:background="@drawable/bg_config_option2"
        app:layout_constraintStart_toStartOf="@id/admin_home_1to1_ib"
        app:layout_constraintTop_toBottomOf="@id/admin_home_1to1_ib" />

    <TextView
        android:id="@+id/admin_home_report_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="14sp"
        android:text="신고 내역"
        android:textStyle="bold"
        android:textColor="@color/black"
        android:layout_marginStart="20dp"
        app:layout_constraintTop_toTopOf="@id/admin_home_report_ib"
        app:layout_constraintBottom_toBottomOf="@id/admin_home_report_ib"
        app:layout_constraintStart_toStartOf="@id/admin_home_report_ib"/>

    <View
        android:layout_width="303dp"
        android:layout_height="1px"
        android:background="@color/gray40"
        app:layout_constraintStart_toStartOf="@id/admin_home_report_ib"
        app:layout_constraintEnd_toEndOf="@id/admin_home_report_ib"
        app:layout_constraintBottom_toBottomOf="@id/admin_home_report_ib"/>

    <ImageButton
        android:id="@+id/admin_home_member_manage_ib"
        android:layout_width="320dp"
        android:layout_height="63dp"
        android:background="@drawable/bg_config_option3"
        app:layout_constraintStart_toStartOf="@id/admin_home_report_ib"
        app:layout_constraintTop_toBottomOf="@id/admin_home_report_ib" />

    <TextView
        android:id="@+id/admin_home_menber_manage_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="14sp"
        android:text="회원 괸리"
        android:textStyle="bold"
        android:textColor="@color/black"
        android:layout_marginStart="20dp"
        app:layout_constraintTop_toTopOf="@id/admin_home_member_manage_ib"
        app:layout_constraintBottom_toBottomOf="@id/admin_home_member_manage_ib"
        app:layout_constraintStart_toStartOf="@id/admin_home_member_manage_ib"/>


</androidx.constraintlayout.widget.ConstraintLayout>